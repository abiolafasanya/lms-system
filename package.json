{
  "name": "lms-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=./src/prisma/schema.prisma && next build",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:init": "prisma init",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "start": "next start",
    "gen:key": "openssl rand -base64 32",
    "lint": "next lint && tsc --noEmit",
    "format": "npx prettier --write .",
    "prepare": "husky install",
    "check-format": "prettier --check .",
    "check-all": "npm run check-format && npm run lint && npm run build"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@clerk/nextjs": "^4.26.2",
    "@hookform/resolvers": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.5.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.7.0",
    "@tanstack/react-table": "^8.9.7",
    "@types/node": "^20.8.10",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@uploadthing/react": "^5.5.0",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint-config-next": "13.4.19",
    "lucide-react": "^0.274.0",
    "next": "^13.5.6",
    "next-themes": "^0.2.1",
    "postcss": "8.4.29",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.46.1",
    "react-icons": "^4.10.1",
    "react-quill": "^2.0.0",
    "react-spinners": "^0.13.8",
    "recharts": "^2.9.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^5.5.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.53.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prisma": "5.5.2",
    "typescript": "^5.2.2"
  }
}
